{
  "name": "gym-management",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:android": "ng build --prod --output-path dist/android/stay-fit --base-href ./",
    "build:prod": "ng build --prod --output-path dist/prod/stay-fit --base-href /stay-fit/",
    "build:qa": "ng build --configuration qa --output-path dist/qa/stay-fit --base-href /stay-fit/",
    "copy:prodhtaccess": "cp src/.htaccess dist/prod/stay-fit/.htaccess",
    "copy:qahtaccess": "cp src/.htaccess dist/qa/stay-fit/.htaccess",
    "prebundle:prod": "rm -rf dist/prod/stay-fit/ && echo 'PROD build is about to start soon...'",
    "bundle:prod": "npm run build:prod && npm run copy:prodhtaccess",
    "postbundle:prod": "echo 'PROD build success'",
    "prebundle:qa": "rm -rf dist/qa/stay-fit/ && echo 'QA build is about to start soon...'",
    "bundle:qa": "npm run build:qa && npm run copy:qahtaccess",
    "postbundle:qa": "echo 'QA build success'",
    "prebundle:android": "rm -rf dist/android/stay-fit/ && echo 'ANDROID build(PROD) is about to start soon...'",
    "bundle:android": "npm run build:android",
    "postbundle:android": "echo 'ANDROID build(PROD) build success'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "^8.0.0",
    "@angular/animations": "~11.2.1",
    "@angular/cdk": "~10.2.4",
    "@angular/common": "~11.2.1",
    "@angular/compiler": "~11.2.1",
    "@angular/core": "~11.2.1",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~11.2.1",
    "@angular/material": "^10.2.4",
    "@angular/platform-browser": "~11.2.1",
    "@angular/platform-browser-dynamic": "~11.2.1",
    "@angular/router": "~11.2.1",
    "@fullcalendar/angular": "^5.1.0",
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/interaction": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@ngx-formly/core": "^5.0.0",
    "@ngx-formly/material": "^5.10.14",
    "@ngx-formly/schematics": "^5.10.14",
    "@syncfusion/ej2-angular-grids": "^18.2.44",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.0",
    "gsap": "2.1.0",
    "gtoken": "^5.0.1",
    "jquery": "^3.5.1",
    "material-icons": "^0.6.1",
    "moment": "^2.29.1",
    "ng-multiselect-dropdown": "^0.2.10",
    "ng-scroll-calendar": "^1.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "~11.2.1",
    "@angular/language-service": "~11.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "schematics-scss-migrate": "^1.1.7",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.5"
  }
}
